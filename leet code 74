public boolean searchMatrix(int[][] m, int t) {
        for(int i=0;i<m.length;i++){
            if(m[i][0]<=t&&m[i][m[0].length-1]>=t){
                for(int j=0;j<m[0].length;j++){
                    if(m[i][j]==t){
                        return true;
                    }
                }
                return false;
            }
        }
        return false;
        }
